@model Blood.Models.CSVUpdate

@{
    ViewData["Title"] = "CSV";
}

<h2>CSV</h2>

<h4>CSVUpdate</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CSV" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CSVUrl" class="control-label"></label>
                <input asp-for="CSVUrl"  type="file" />
                <span asp-validation-for="CSVUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
