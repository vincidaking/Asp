<div class="jumbotron table-wrapper-scroll-y my-custom-scrollbar" id="News" data-spy="scroll">
    @model List<Zajecia3v2.Models.RSSItem>

    @{
        ViewBag.Title = "Index";
    }


    @foreach (var Item in Model)
    {

        <h3>@Item.Title </h3>
        <span>@Item.PubDate </span>
        <p>@Html.Raw(Item.Description) </p>
        <p><a href="@Item.Link" target="_blank">@Item.Link</a></p>
    }

    
</div>

